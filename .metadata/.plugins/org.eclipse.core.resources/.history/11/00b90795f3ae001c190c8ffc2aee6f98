package com.jiaxun.library.book;

public class BookBuilder {
	
	private String booknames;
	private String authors;
	private String pubdates;
	private int sumpaginations;
	public BookBuilder setBooknames(String booknames) {
		this.booknames = booknames;
		return this;
	}
	public BookBuilder setAuthors(String authors) {
		this.authors = authors;
		return this;
	}
	public BookBuilder setPubdates(String pubdates) {
		this.pubdates = pubdates;
		return this;
	}
	public BookBuilder setSumpaginations(int sumpaginations) {
		this.sumpaginations = sumpaginations;
		return this;
	}
	
	public Book getBook() {
		return new Book(booknames, authors, pubdates, sumpaginations);
	}
}
